<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <corners android:radius="10sp" />
    <gradient
        android:angle="45"
        android:centerColor="#669F4B"
        android:centerX="35%"
        android:endColor="#669F4B"
        android:startColor="#669F4B"
        android:type="linear" />
    <padding
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp" />
    <size
        android:width="270dp"
        android:height="40dp" />
    <stroke
        android:width="1sp"
        android:color="#3B6E22" />
</shape>